{
  "name": "cordova-testbed",
  "version": "0.3.0",
  "license": "Apache-2.0",
  "description": "Use medic to test a cordova plugin locally",
  "main": "lib/paramedic.js",
  "bin": {
    "cordova-paramedic": "./main.js",
    "cordova-testbed": "./testbed/cli.js"
  },
  "engines": {
    "node": ">= 8"
  },
  "repository": "ratson/cordova-testbed",
  "scripts": {
    "test": "node testbed/cli.js --platform ios --plugin ./spec/testable-plugin/ --verbose",
    "posttest": "run-s lint:*",
    "lint": "run-s lint:*",
    "lint:eslint": "eslint testbed",
    "lint:jshint": "jshint lib/",
    "test-appveyor": "npm run test-windows",
    "test-travis": "npm run test-ios",
    "test-android": "node main.js --platform android --plugin ./spec/testable-plugin/",
    "test-ios": "node main.js --platform ios --plugin ./spec/testable-plugin/ --verbose",
    "test-windows": "node main.js --platform windows --plugin ./spec/testable-plugin/",
    "test-browser": "node main.js --platform browser --plugin ./spec/testable-plugin/"
  },
  "keywords": [
    "cordova",
    "test",
    "plugin",
    "cli",
    "medic",
    "paramedic",
    "testbed",
    "ecosystem:cordova"
  ],
  "author": "Ratson",
  "dependencies": {
    "cordova-common": "^3.0.0",
    "execa": "^1.0.0",
    "expect-telnet": "^1.0.0",
    "jasmine": "3.3.1",
    "jasmine-reporters": "^2.3.2",
    "jasmine-spec-reporter": "^4.2.1",
    "localtunnel": "~1.9.1",
    "make-promises-safe": "^4.0.0",
    "minimist": "~1.2.0",
    "ms": "^2.1.1",
    "path-extra": "^4.2.1",
    "q": "^1.5.1",
    "randomstring": "^1.1.5",
    "sauce-connect-launcher": "^1.2.4",
    "saucelabs": "^1.5.0",
    "shelljs": "~0.6.1",
    "signale": "^1.3.0",
    "socket.io": "^2.2.0",
    "tcp-port-used": "^1.0.1",
    "tmp": "0.0.33",
    "tree-kill": "^1.2.1",
    "unorm": "^1.4.1",
    "wd": "^1.11.1",
    "yargs": "^12.0.5"
  },
  "devDependencies": {
    "eslint": "^5.10.0",
    "eslint-config-concise": "^0.35.0",
    "jshint": "^2.9.7",
    "npm-run-all": "^4.1.5"
  },
  "eslintConfig": {
    "extends": [
      "concise"
    ]
  }
}
